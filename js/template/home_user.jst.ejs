<% if(isLogined){ %>
    <div class="logined">
       <div class="avatar"><img src="<%= userFace %>" width="50" height="50"/><span class="cover"></span></div>
       <div class="user-info"><span class="user-name"><%= userNick %></span><span class="weather" id="J-weather"><img src="http://a.tbcdn.cn/mw/app/youai/h5/v3/img/icon/<%= weatherIcon %>.png"/><em><%= temperature %></em></span></div>
       <p class="login-info"><%= loginInfo %></p>
    </div>
<% }else{ %>
    <div class="unlogin"><%= loginInfo %></div>
<% } %>
</div>
